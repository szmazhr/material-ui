    "@types/lodash.get": "^4.4.9",
    "@types/mocha": "^10.0.4",
    "@types/node": "^18.18.10",
    "@types/stylis": "^4.2.4"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
